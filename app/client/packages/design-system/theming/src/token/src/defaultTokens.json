{
  "seedColor": "#0080ff",
  "colorMode": "light",
  "borderRadiusElevation": {
    "base": "0px",
    "1": "calc(var(--border-radius-elevation-base) + var(--outer-spacing-1) * 2)",
    "2": "calc(var(--border-radius-elevation-base) + var(--outer-spacing-1))",
    "3": "var(--border-radius-elevation-base)"
  },
  "boxShadow": {
    "1": "0 var(--inner-spacing-2) var(--inner-spacing-3) 0 var(--color-shadow-elevation-1)",
    "2": "0 var(--inner-spacing-1) var(--inner-spacing-3) 0 var(--color-shadow-elevation-2)",
    "3": "0 calc(var(--inner-spacing-1)/2) var(--inner-spacing-3) 0 var(--color-shadow-elevation-3)"
  },
  "borderWidth": {
    "1": "1px",
    "2": "2px"
  },
  "opacity": {
    "disabled": 0.3
  },
  "zIndex": {
    "1": 3,
    "2": 4,
    "3": 10,
    "99": 9999
  }
}
